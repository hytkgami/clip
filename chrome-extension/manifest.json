{
  "name": "Clip",
  "description": "Clip web pages to your Clipper App",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Ctrl+Shift+P"
      }
    }
  },
  "action": {
    "default_popup": "popup.html"
  }
}
